/* pages/cartoon/cartoon.wxss */
/*自定义导航*/
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  border-bottom:1px #f0f0f0 solid;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  color: #fff;
  font-size:30rpx;
  letter-spacing:2px;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-green{
  background-color: #65ab31;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}
/***********************************/
/*板块容器*/
.content{
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 45rpx auto;
  width: 682rpx;
  flex-direction: column;
}
/*板块样式*/
.cartoonImg{
  width: 682rpx;
  margin-bottom: 30rpx;
  box-shadow: 1px 1px 5px rgba(0, 0, 0, 0.2);
}