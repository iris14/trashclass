/* pages/softintroduce/softintroduce.wxss */
/*自定义导航*/
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  color: #fff;
  font-size:28rpx;
  letter-spacing:2px;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-green{
  background-color: #65ab31;
}
.overflow{
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}
/* 背景颜色 */
.bg{
  background-color: #F3F3F3
}
.hidden{
  overflow: hidden;
}
/* banner的样式 */
.banner{
  width: 100%;
  /* height: 400rpx; */
}
/* 大模块的样式 */
.box{
  border: 1rpx solid #E9E9E9;
  background-color:#fff;
  width: 630rpx;
  margin-left: 28rpx;
  margin-top: 40rpx;
  border-radius: 15rpx;
  padding: 35rpx;
}
/* 模块中的标题 */
.biao{
  color: #65A92F;
  font-weight: bold;
  font-size: 35rpx;
  display: inline-block;
  /* padding-bottom: -25rpx; */
}
/* 模块中的小文字 */
.wen{
  font-size: 30rpx;
  border-bottom: 1px dashed #65ab31;
  margin-top: 2rpx;
  padding: 8rpx;
  /* text-indent: 2em; */
}
/* 所有图片的样式 */
.all-image{
  width: 60rpx;
  height: 60rpx;
  /* border-bottom: 1px dashed #65ab31; */
  float: left;
  margin-right: 5rpx;
}
.display{
  display: inline-block;
  position: relative;
  top: 16rpx;
  left: 5rpx;
}
/* 底部文字的样式 */
.bottom-wei{
  color: #65AB31;
  margin-top: 90rpx;
  margin-bottom: 70rpx;
  font-size: 30rpx;
  
}
/* 底部文字居中 */
.bottom-center{
  display: flex;
  justify-content: center;
}
