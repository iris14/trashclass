/* pages/feedback/feedback.wxss */
/*自定义导航*/
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  color: #fff;
  font-size:28rpx;
  letter-spacing:2px;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-green{
  background-color: #65ab31;
}
.overflow{
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}
.hidden{
  overflow: hidden;
}


/* 页面背景色 */
page{
  background-color: #f0f0f0;
}
/* 每一行的共同样式 */
.all-line{
  width: 100%;
  /* height: 76rpx; */
  background-color: #fff;
  margin-bottom: 15rpx;
}
/* 每个文本前的图标的共同样式 */
.all-texticon{
  width: 55rpx;
  height: 55rpx;
  display: inline-block;
  position: relative;
  top: 15rpx;
  margin: 0 20rpx 0 40rpx;
}
/* 每个文本的共同样式 */
.all-text{
  display: inline-block;
  color: #333;
  line-height: 76rpx;
}
/* 每个go图标的共同样式 */
.all-goicon{
  width: 18rpx;
  height: 28rpx;
  float: right;
  margin: 30rpx 20rpx 0 0 ;
}