/* pages/userfeedback/userfeedback.wxss */
/*自定义导航*/
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  color: #fff;
  font-size:28rpx;
  letter-spacing:2px;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-green{
  background-color: #65ab31;
}
.overflow{
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}
.hidden{
  overflow: hidden;
}


page{
  background-color: #f0f0f0;
  font-size: 40rpx;
}
.new{
  color: #999;
  font-family: "幼圆";
  margin: 45rpx 0 15rpx 20rpx;
  font-size: 35rpx;
}
.input-title, .input-contact, .input-context{
  height: 80rpx;
  border-top: solid 1rpx rgb(220, 219, 219);
  background-color: #fff;
  padding-left: 20rpx;
}
.input-context{
  width: 100%;
  height: 350rpx;
  border-bottom: solid 1rpx rgb(220, 219, 219);
  padding: 40rpx 0 0 20rpx;
}
.note{
  text-align: center;
  color: rgb(206, 202, 202);
  font-size: 30rpx;
  margin-top: 30rpx;
}
.submit-btn{
  width: 700rpx;
  height: 100rpx;
  position: absolute;
  bottom: 50rpx;
  left: 2%;
}